<h1>List Products</h1>
<hr>
<%= link_to "Add Product", new_product_path %>
 <table>
    <tr>
      <th>Gambar</th>
      <th>id</th>
      <th>Name</th>
      <th>Description</th>
      <th>Price</th>
      <th>Weight</th>
      <th>User Id</th>
      <th>Category</th>
      <th>Manage</th>
    </tr>
  
    <% @products_paging.each do |product| %>
      <tr>
        <td><%= image_tag product.photo.url %></td>
        <td><%= product.id %></td>
        <td><%= product.name %></td>
        <td><%= product.description %></td>
        <td><%= product.price %></td>
        <td><%= product.weight %></td>
        <td><%= product.user.name %></td>
        <td><%= product.category.name %></td>
      
     
        <td><%= link_to "Show" ,product_path(product.id) %> |<%= link_to "Edit", edit_product_path(product.id) %> | <%= link_to "Delete", product_path(product.id), confirm:'Delete This Product ?',method: :delete %> </td>
        </tr>
    <% end %>
  
  </table>
<%= will_paginate @products_paging %>